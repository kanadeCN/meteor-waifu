{
  "schemaVersion": 1,
  "id": "meteorwaifu",
  "version": "${version}",
  "name": "MeteorWaifu",
  "description": "An addon for Meteor Client which adds lot's of anime girls.",
  "authors": [
    "kanadeCN"
  ],
  "contact": {
    "repo": "https://github.com/kanadeCN/meteor-waifu"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "kanadecn.addon.meteorwaifu.MeteorWaifu"
    ]
  },
  "mixins": [
    "meteorwaifu.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "248,168,209"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
